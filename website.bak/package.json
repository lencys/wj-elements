    "@mdx-js/react": "^1.6.22",
    "@stackblitz/sdk": "^1.6.0",
    "@tippyjs/react": "^4.2.6",
    "@uiw/react-codesandbox": "^1.1.5",
    "clsx": "^1.2.1",
    "concurrently": "^6.2.0",
    "crowdin": "^3.5.0",
    "docusaurus-plugin-code-preview": "^0.0.1",
    "docusaurus-plugin-module-alias": "^0.0.2",
    "docusaurus-plugin-sass": "^0.2.5",
    "fs-extra": "^9.1.0",
    "jsx-runtime": "^1.2.0",
    "modern-normalize": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-live": "^3.2.0",
